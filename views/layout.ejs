<script type="text/javascript">
  $(".button-collapse").sideNav();
  check_scroll_top();
  $(window).scroll(function () {
    check_scroll_top();
  });
  function check_scroll_top(){
    if ($(window).scrollTop() > 1400) {
      $("#back-to-top").fadeIn(400);
    } else {
      $("#back-to-top").fadeOut(500);
    }
  }
</script>
