    <script type="text/javascript">
    function encode(urrl){
      var newurl="";
      for(var i=0;i<urrl.length;i++){
        var c=urrl[i];
        if(c=='/')
          newurl+="%2F";
        else
          newurl+=c;
      }
      return newurl;
    }
  </script>
