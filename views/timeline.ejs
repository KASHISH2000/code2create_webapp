<script type="text/javascript">
  $("#day2").hide();
  $("#day3").hide();
  $("#navday1").click(function(){
    $("#navday1").addClass('active');
    $("#navday2").removeClass('active');
    $("#navday3").removeClass('active');

    $("#day1").show();
    $("#day2").hide();
    $("#day3").hide();
  });
  $("#navday2").click(function(){
    $("#navday2").addClass('active');
    $("#navday1").removeClass('active');
    $("#navday3").removeClass('active');

    $("#day2").show();
    $("#day1").hide();
    $("#day3").hide();
  });
  $("#navday3").click(function(){
    $("#navday3").addClass('active');
    $("#navday1").removeClass('active');
    $("#navday2").removeClass('active');

    $("#day3").show();
    $("#day1").hide();
    $("#day2").hide();
  });
</script>
