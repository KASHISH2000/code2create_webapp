<script type="text/javascript">
  $("#teamid").on("keydown", function (e) {
    return (e.which !== 32) && (e.which!== 35) && (e.which!== 36) && (e.which!== 37) && (e.which!== 38) ;
  });
</script>
<script type="text/javascript">
  var cur_rank=0;
  var rank1=false;
  var rank2=false;
  var rank3=false;
  var rank4=false;
  function get_rank()
  {
    if(!rank1)
    {
      rank1=true;
      return 1;
    }
    if(!rank2)
    {
      rank2=true;
      return 2;
    }
    if(!rank3)
    {
      rank3=true;
      return 3;
    }
    if(!rank4)
    {
      rank4=true;
      return 4;
    }
    return 0;
  }
  $(".track-col").click(function(){
    if($(this).hasClass('selected'))
    {
      $(this).removeClass('selected');
      var str_val=$(this).find('.rank-disp').text();
      $(this).find('.inp_track').val("0");
      console.log("The value is:"+str_val);
      if(str_val=="1")
        rank1=false;
      if(str_val=="2")
        rank2=false;
      if(str_val=="3")
        rank3=false;
      if(str_val=="4")
        rank4=false;
    }
    else
    {
      $(this).addClass("selected");
      var r=get_rank();
      $(this).find('.rank-disp').html(r);
      $(this).find('.inp_track').val(r);
    }
  });
</script>

