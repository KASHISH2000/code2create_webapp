 <script type = "text/javascript">
  $(function() {

    $('.kc-wrap').KillerCarousel({
                   // Width of carousel.
                   width: 800,
                   // Item spacing in 3d (CSS3 3d) mode.
                   spacing3d: 300,
                   // Item spacing in 2d (no CSS3 3d) mode. 
                   spacing2d: 300,
                   // Path to images for shadows etc.
                   imagePath: "/sites/starplugins/js/killercarousel/",
                   showShadow: true,
                   showReflection: true,
                   // Looping mode.
                   infiniteLoop: false,
                   // Scale at 75% of parent element.
                   autoScale:80,
                   showNavigation: true,
                   frontItemIndex:1

                 });

    var kc = $('.kc-wrap').data('KillerCarousel');



    setInterval( function(){
      var i = kc.getFrontItemIndex();
      var $ele = $('img',kc.getItemElement(i));
      $('#plugin-name').text( $ele.attr('title'));
      $('#plugin-description').text( $ele.attr('data-desc'));
    },250);
  });
</script>

